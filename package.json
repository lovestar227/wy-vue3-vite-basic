  "scripts": {
    "dev": "cross-env --max_old_space_size=4096 vite",
    "preview": "vite preview --port 8089",
    "build": "rimraf dist && cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:preview": "pnpm build && vite preview --port 8089",
    "build:staging": "rimraf dist && cross-env vite build --mode staging",
    "clean": "rimraf node_modules && rimraf pnpm-lock.yaml && pnpm install",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,css,scss,sass,less}\" ",
    "prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\" \"!src/assets/**\" \"types/**/*.{ts,md}\" \"build/**/*.{ts,md}\"",
    "lint-staged": "lint-staged -c ./.husky/.lintstagedrc.cjs",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "browserslist": [
    "ie >= 11",
    "last 2 versions",
    "> 1%"
  ],
